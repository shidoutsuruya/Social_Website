    "version": "0.2.0",
    "configurations": [



        {
            "name": "Django runserver 1",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver",
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Django runserver 2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver_plus",
                "--cert-file",
                "cert.crt"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Django shell",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "shell",
            ],
            "django": true,
            "justMyCode": true
        }
        {
            "name": "Django migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "migrate"
            ],
            "django": true,
            "justMyCode": true
        }
    ]
}